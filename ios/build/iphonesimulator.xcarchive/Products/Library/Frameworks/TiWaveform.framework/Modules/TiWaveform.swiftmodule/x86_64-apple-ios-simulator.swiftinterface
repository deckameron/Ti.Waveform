public enum WaveformMode {
  case linear
  case circular
  public static func == (a: TiWaveform.WaveformMode, b: TiWaveform.WaveformMode) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public enum WaveformState {
  case idle
  case loading
  case ready
  case playing
  case recording
  case paused
  case error(Swift.String)
}
public protocol WaveformViewDelegate : AnyObject {
  func waveformDidFinishLoading(_ waveform: TiWaveform.WaveformView)
  func waveformDidUpdateProgress(_ waveform: TiWaveform.WaveformView, progress: CoreFoundation.CGFloat)
  func waveformDidStartRecording(_ waveform: TiWaveform.WaveformView)
  func waveformDidStopRecording(_ waveform: TiWaveform.WaveformView)
  func waveformDidEncounterError(_ waveform: TiWaveform.WaveformView, error: Swift.String)
  func waveformDidSeek(_ waveform: TiWaveform.WaveformView, to progress: CoreFoundation.CGFloat)
  func waveformDidPauseRecording(_ waveform: TiWaveform.WaveformView)
  func waveformDidResumeRecording(_ waveform: TiWaveform.WaveformView)
}
