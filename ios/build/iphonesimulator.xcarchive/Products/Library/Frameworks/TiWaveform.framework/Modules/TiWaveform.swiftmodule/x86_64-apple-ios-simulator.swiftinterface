public enum SamplingError : Swift.Error {
  case fileNotFound
  case unsupportedFormat
  case decodingFailed
  case invalidBuffer
  case bufferCreationFailed
  public static func == (a: TiWaveform.SamplingError, b: TiWaveform.SamplingError) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public enum SamplingMode {
  case playback(targetSamples: Swift.Int)
  case recording(windowSize: Swift.Int)
}
public class AmplitudeSampler {
  public init(mode: TiWaveform.SamplingMode)
  #if compiler(>=5.3) && $NonescapableTypes
  public func sampleFile(at url: Foundation.URL, targetSamples: Swift.Int? = nil) throws -> [Swift.Float]
  #endif
  public func processLiveBuffer(_ buffer: AVFAudio.AVAudioPCMBuffer) throws -> [Swift.Float]
  @objc deinit
}
public enum AudioFormat {
  case aac
  case mp3
  case wav
  case m4a
  case opus
  case ogg
  case unknown
  public static func == (a: TiWaveform.AudioFormat, b: TiWaveform.AudioFormat) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
@_hasMissingDesignatedInitializers public class AudioDecoder {
  public static func decode(url: Foundation.URL) throws -> AVFAudio.AVAudioPCMBuffer
  @objc deinit
}
public enum NormalizationMode {
  case global
  case dynamic
  public static func == (a: TiWaveform.NormalizationMode, b: TiWaveform.NormalizationMode) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public class WaveformModel {
  public var minAmplitude: Swift.Float
  public var maxAmplitude: Swift.Float
  public var mode: TiWaveform.NormalizationMode
  public init(mode: TiWaveform.NormalizationMode = .global)
  public func update(amplitudes: [Swift.Float])
  public func append(amplitude: Swift.Float)
  public func clear()
  public func amplitude(at index: Swift.Int) -> Swift.Float
  public func amplitudes(in range: Swift.Range<Swift.Int>) -> [Swift.Float]
  public var count: Swift.Int {
    get
  }
  public var isEmpty: Swift.Bool {
    get
  }
  public var averageAmplitude: Swift.Float {
    get
  }
  public var peakAmplitude: Swift.Float {
    get
  }
  @objc deinit
}
