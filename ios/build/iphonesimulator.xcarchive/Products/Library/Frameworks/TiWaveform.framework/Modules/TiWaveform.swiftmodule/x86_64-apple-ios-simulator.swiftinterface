    get
    set
  }
  @_Concurrency.MainActor @preconcurrency public var linearConfiguration: TiWaveform.LinearWaveformConfiguration {
    get
    set
  }
  @_Concurrency.MainActor @preconcurrency public var circularConfiguration: TiWaveform.CircularWaveformConfiguration {
    get
    set
  }
  @_Concurrency.MainActor @preconcurrency @objc override dynamic public init(frame: CoreFoundation.CGRect)
  #if compiler(>=5.3) && $NonescapableTypes
  @_Concurrency.MainActor @preconcurrency @objc required dynamic public init?(coder: Foundation.NSCoder)
  #endif
  @_Concurrency.MainActor @preconcurrency public func loadAudio(from url: Foundation.URL, targetSamples: Swift.Int = 200)
  @_Concurrency.MainActor @preconcurrency public func setProgress(_ progress: CoreFoundation.CGFloat, animated: Swift.Bool = false)
  @_Concurrency.MainActor @preconcurrency public func startRecording()
  @_Concurrency.MainActor @preconcurrency public func stopRecording()
  @_Concurrency.MainActor @preconcurrency public func pauseRecording()
  @_Concurrency.MainActor @preconcurrency public func resumeRecording()
  @_Concurrency.MainActor @preconcurrency public func clear()
  @_Concurrency.MainActor @preconcurrency public func updateColors(activeColor: UIKit.UIColor, inactiveColor: UIKit.UIColor, animated: Swift.Bool = true)
  @objc deinit
}
