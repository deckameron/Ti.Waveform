public protocol LiveWaveformRecorderDelegate : AnyObject {
  func recorderDidUpdateAmplitude(_ recorder: TiWaveform.LiveWaveformRecorder, amplitude: Swift.Float)
  func recorderDidEncounterError(_ recorder: TiWaveform.LiveWaveformRecorder, error: Swift.String)
}
@_hasMissingDesignatedInitializers public class LiveWaveformRecorder {
  weak public var delegate: (any TiWaveform.LiveWaveformRecorderDelegate)?
  public func startRecording() throws
  public func stopRecording()
  public func pauseRecording()
  public func resumeRecording()
  @objc deinit
}
public struct LinearWaveformConfiguration {
  public init()
}
